<h2>Nova loja</h2>

<%= simple_form_for([:admin, @store]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= render partial: 'form_inputs', locals: { f: f } %>
  </div>
  
  <div class="form-actions">
    <%= f.button :submit, "Criar", class: 'btn btn-primary' %>
  </div>
<% end %>